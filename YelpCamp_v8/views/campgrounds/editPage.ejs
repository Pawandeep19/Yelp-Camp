<%- include("../partials/header.ejs") %>
<h1>Edit <%=foundcamp.name %></h1>
<form method="post" action="/campground/<%=foundcamp._id %>?_method=PUT">
     <input type="text" name="campground[name]" value="<%=foundcamp.name %>">
     <input type="text" name="campground[image]" value="<%=foundcamp.image %>">
     <input type="text" name="campground[description]" value="<%=foundcamp.description %>">
     <button>SUBMIT</button>
</form>

